{
    "name": "wtfz/laravel-route-button",
    "description": "Generate Laravel route button with dropdown from model",
    "keywords": [
        "wtfz",
        "laravel-route-button"
    ],
    "homepage": "https://github.com/wtfz/laravel-route-button",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ahmad Zaim",
            "email": "ahmadzaimhamzah@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Wtfz\\LaravelRouteButton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wtfz\\LaravelRouteButton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wtfz\\LaravelRouteButton\\LaravelRouteButtonServiceProvider"
            ],
            "aliases": {
                "LaravelRouteButton": "Wtfz\\LaravelRouteButton\\LaravelRouteButtonFacade"
            }
        }
    }
}
