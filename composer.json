{
    "name": "wtfz/laravel-route-button",
    "description": "Generate route buttons with dropdown from the model",
    "keywords": [
        "wtfz",
        "laravel-route-button"
    ],
    "homepage": "https://github.com/wtfz/laravel-route-button",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ahmad Zaim",
            "email": "ahmadzaimhamzah@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wtfz\\LaravelRouteButton\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wtfz\\LaravelRouteButton\\RouteButtonServiceProvider"
            ],
            "aliases": {
                "RouteButton": "Wtfz\\LaravelRouteButton\\RouteButtonFacade"
            }
        }
    }
}
